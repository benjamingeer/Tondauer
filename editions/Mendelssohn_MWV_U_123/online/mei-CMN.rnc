         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id =
           "mei-staffDef-Check_clef_position_staffDef-constraint-rule-160"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "mei:staffDef[@clef.line and @lines]"
