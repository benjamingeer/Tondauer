             role = "warning"
             test = "count(mei:label) = count(mei:contentItem)"
             "When labels\x{a}" ~
             "            are used, usually each content item has one."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
