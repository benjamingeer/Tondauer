             test = "ancestor::mei:staff/@n eq $thisstaff"
             "@n must have the same value as the\x{a}" ~
             "            current staff."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id =
           "mei-staffDef-Check_ancestor_staff_lines-constraint-rule-159"
         "\x{a}" ~
         "            "
