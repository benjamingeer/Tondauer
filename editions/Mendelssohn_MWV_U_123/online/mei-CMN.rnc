mei_handList =
  
  ## Container for one or more hand elements.
  element handList {
    mei_model.headLike*,
    ((mei_label?, mei_hand)*)
    >> sch:pattern [
         id = "mei-handList-checkHandListLabels-constraint-rule-67"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "mei:handList[mei:label]"
           "\x{a}" ~
