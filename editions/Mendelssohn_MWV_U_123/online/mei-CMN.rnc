         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id =
           "mei-staffDef-Check_tab_strings_nolines-constraint-rule-163"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "mei:staffDef[@tab.strings and not(@lines)]"
           "\x{a}" ~
           "               "
           sch:let [
             name = "countTokens"
             value =
               "count(tokenize(normalize-space(@tab.strings), '\s'))"
           ]
           "\x{a}" ~
           "               "
