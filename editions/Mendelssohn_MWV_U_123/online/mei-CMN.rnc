           "\x{a}" ~
           "               "
           sch:let [ name = "staffPPQ" value = "@ppq" ]
           "\x{a}" ~
           "               "
           sch:let [
             name = "scorePPQ"
             value = "preceding::mei:scoreDef[@ppq][1]/@ppq"
           ]
           "\x{a}" ~
           "               "
           sch:assert [
             test = "($scorePPQ mod $staffPPQ) = 0"
             "The value of ppq must be a factor of\x{a}" ~
             "              the value of ppq on a preceding scoreDef."
           ]
           "\x{a}" ~
           "            "
